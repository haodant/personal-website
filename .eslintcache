iv ref={mount} />\n      <Quote variants={parent} initial=\"initial\" animate=\"animate\">\n        <Quoteline variants={child}>\n          I am HCI researcher / engineer. \n        </Quoteline>\n      </Quote>\n\n      <BannerTitle variants={parent} initial=\"initial\" animate=\"animate\">\n        <Headline variants={child}>Hi there,</Headline>\n      </BannerTitle>\n    </Banner>\n  );\n};\n\nexport default HomeBanner;\n","/Users/haodant/Documents/personalWebsite/portfolio/src/hooks/useWindowSize.js",[],"/Users/haodant/Documents/personalWebsite/portfolio/src/styles/homeStyles.js",[],"/Users/haodant/Documents/personalWebsite/portfolio/src/components/navigation.js",[],"/Users/haodant/Documents/personalWebsite/portfolio/src/styles/navigationStyles.js",[],"/Users/haodant/Documents/personalWebsite/portfolio/src/components/footer.js",[],"/Users/haodant/Documents/personalWebsite/portfolio/src/styles/footerStyles.js",["99"],"import styled, { css } from 'styled-components'\nimport { motion } from 'framer-motion'\n\nexport const FooterNav = styled.div`\n    position: relative;\n    bottom: 50px;\n    height: 0;\n`\n\nexport const FooterContent= styled.div`\n    color: ${props => props.theme.text};\n    font-size: 0.8rem;\n    font-weight: 400;\n    line-height: .5rem;\n    text-align: center;\n    flex: 1;\n    ${props => props.wider && css`\n        flex: 2;\n    `}\n`","/Users/haodant/Documents/personalWebsite/portfolio/src/components/research.js",["100","101","102","103"],"/Users/haodant/Documents/personalWebsite/portfolio/src/components/projects/projectSummary.js",["104"],"/Users/haodant/Documents/personalWebsite/portfolio/src/styles/projectStyles.js",["105"],"/Users/haodant/Documents/personalWebsite/portfolio/src/components/projects/projectBanner.js",["106"],"/Users/haodant/Documents/personalWebsite/portfolio/src/components/projects/project.data.js",[],{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","severity":1,"message":"112",